			cout << "Dexterity Mod: " << DEXmod << endl;
			cout << "Constitution Mod: " << CONmod << endl;
			cout << "Intelligence Mod: " << INTmod << endl;
			cout << "Wisdom Mod: " << WISmod << endl;
			cout << "Charisma Mod: " << CHAmod << endl;

		}
		int strGet(){
