			cout << "Your name is " << name;
			cout << " and your gender is " << gender << "." << endl;
		}
		void playerclassSet(string aPlayerClass){
			int x;
			while(x < 13){
				if(playerClass[x] == "0"){
					playerClass[x] = aPlayerClass;
					break;
				}
				x++;
			}
		}
		void playerclassGet(){
			cout << "Your classes are:" << endl;
			for(int x = 0; x < 13; x++){
				if (playerClass[x] != "0")
					cout << "[" << x + 1 << "] - " << playerClass[x] << endl;
			}
		}
